{
  "name": "awrtc_signaling",
  "version": "1.1.2",
  "description": "Default ws based signaling server for the unity asset WebRtcVideoChat, awrtc_browser and awrtc_python",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --project tsconfig.vitest.json --noEmit",
    "typecheck:watch": "tsc --project tsconfig.vitest.json --noEmit --watch",
    "start": "node out/server.js",
    "watch": "tsx watch src/server.ts",
    "lint": "eslint",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/because-why-not/awrtc_signaling.git"
  },
  "keywords": [
    "webrtc",
    "signaling",
    "awrtc",
    "unity",
    "WebRtcVideoChat"
  ],
  "author": "Christoph Kutza",
  "email": "contact@because-why-not.com",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/because-why-not/awrtc_signaling/issues"
  },
  "homepage": "https://github.com/because-why-not/awrtc_signaling#readme",
  "dependencies": {
    "@types/finalhandler": "^1.2.4",
    "@types/serve-static": "^1.15.8",
    "finalhandler": "^1.3.1",
    "serve-static": "^2.2.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^22.15.32",
    "@types/ws": "^8.18.1",
    "eslint": "^9.31.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  }
}
